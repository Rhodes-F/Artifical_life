            <origin xyz="0 0 -0.5" rpy="0 0 0"/>
            <geometry>
                <box size="0.3 0.3 1" />
            </geometry>
        </collision>
    </link>
    <joint name="Torso_RightArm" type="revolute">
       <parent link="Torso"/>
       <child  link="RightArm"/>
