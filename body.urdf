                <color rgba="0 1.0 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 -0.5" rpy="0 0 0"/>
            <geometry>
                <box size="0.3 0.3 1" />
            </geometry>
        </collision>
    </link>
    <joint name="Torso_BackArm" type="revolute">
       <parent link="Torso"/>
