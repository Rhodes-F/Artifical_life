            <geometry>
                <box size="0.3 0.3 1" />
            </geometry>
        </collision>
    </link>
    <joint name="Torso_LeftArm" type="revolute">
       <parent link="Torso"/>
       <child  link="LeftArm"/>
       <origin rpy="0 0 0" xyz="0 0.5 3" />
     <axis xyz="1 0 0"/>
       <limit effort="0.0" lower="-3.14159" upper="3.14159" velocity="0.0"/>
